{
    "message": {
        "__type": "string",
        "__omitEmpty": false,
        "__maxLen": 100,
        "__mandatory": "message"
    },
    "timestamp": {
        "__type": "time",
        "__fromCaller": true,
        "__timeFormat": "2006-01-02T15:04:05.999999999Z07:00",
        "__mandatory": "timestamp"
    },
    "caller": {
        "__type": "caller",
        "__maxLen": 100,
        "__mandatory": "caller"
    },
    "severity": {
        "__type": "string",
        "__maxLen": 10,
        "__mandatory": "loglevel"
    },
    "version": {
        "__type": "string",
        "__omitEmpty": true,
        "__maxLen": 100,
        "__required": false,
        "__apiAlias": "Version"
    },
    "counter": {
        "__type": "int",
        "__required": false,
        "__apiAlias": "Counter"
    },
    "data": {
        "url": {
            "__type": "string",
            "__omitEmpty": true,
            "__maxLen": 100,
            "__required": false,
            "__apiAlias": "IndexUrl"
        },
        "request": {
            "args": {
                "__type": "string",
                "__omitEmpty": true,
                "__maxLen": 100,
                "__fromEnv": "REQ_ARGS",
                "__apiAlias": "Args"
            },
            "userID": {
                "__type": "int",
                "__fromEnv": "USERID",
                "__apiAlias": "UserId"
            }
        }
    }
}